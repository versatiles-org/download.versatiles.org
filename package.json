    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "work-faster": "^2.5.3"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/eslint__js": "^9.14.0",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
