    "cookie": "^1.1.1",
    "express": "^5.2.1",
    "handlebars": "^4.7.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "work-faster": "^2.5.5"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "@versatiles/release-tool": "^2.6.0",
